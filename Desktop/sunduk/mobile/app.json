{
  "expo": {
    "name": "Sunduk - Turkish Learning",
    "slug": "sunduk-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#6949ff",
      "resizeMode": "contain"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Nunito-Regular.ttf",
            "./assets/fonts/Nunito-Medium.ttf",
            "./assets/fonts/Nunito-SemiBold.ttf",
            "./assets/fonts/Nunito-Bold.ttf"
          ]
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sunduk.turkishlearning"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#6949ff"
      },
      "package": "com.sunduk.turkishlearning"
    }
  }
}

